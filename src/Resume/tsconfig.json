{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "system",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noLib": false,
    "outDir": "./wwwroot/js/app/",
    "removeComments": false,
    "rootDir": "./Scripts",
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": ["node_modules", "wwwroot"]
}