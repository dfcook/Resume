{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "system",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noLib": false,
    "outDir": "./build",
    "removeComments": false,
    "rootDir": "./Scripts",
    "inlineSourceMap": false,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [ "node_modules", "wwwroot", "typings" ]
}